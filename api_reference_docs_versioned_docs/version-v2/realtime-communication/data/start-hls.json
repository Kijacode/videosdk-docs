{
  "startHLSstream": {
    "description": "This API let you start an HLS stream by passing roomId and config object as body parameters.",
    "headers": [
      {
        "key": "Authorization",
        "value": "$YOUR_TOKEN"
      },
      {
        "key": "Content-Type",
        "value": "application/json"
      }
    ],
    "methodType": "POST",
    "postParameters": [
      {
        "key": "roomId",
        "value": "xyz",
        "description": "RoomId for which the hlsstream is to be started",
        "required": true
      },
      {
        "key": "Config",
        "value": "layout: {'type': 'SPOTLIGHT', 'priority': 'PIN', 'gridSize': 5}",
        "description": "Your config object",
        "required": true
      }
    ],
    "queryParameters": [],
    "parameters": [],
    "apiEndpoint": "https://api.videosdk.live/v2/hls/start",
    "response": {
      "downstreamUrl": "some_url"
    }
  }
}
